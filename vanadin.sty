% Populate the following macro within your document to populate the title page
\newcommand{\maketitlepage}[3]{
\begin{titlepage}
\begin{center}
  \titlefont\@subject
  \vspace{2cm}

  \@publishers

  #1

  \Huge\@title

  #2
\end{center}

\vfill
\begin{multicols}{2}
  #3
\end{multicols}
	
\end{titlepage}
%\pagestyle{scrheadings}
}


